{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-console": "off",
      "linebreak-style": 0,
      "max-len": 0,
      "react/jsx-filename-extension": 0,
      "no-alert": 0,
      "react/prop-types": 0
    }
}